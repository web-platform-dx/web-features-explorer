{% if feature.interop.length %}
  <div class="interop feature-box">
    <h3>Interop</h3>
    <ul>
      {% for iteration in feature.interop %}
        <li>
          Included in <a href="{{ iteration.url }}">Interop {{ iteration.year }}</a>
        </li>
      {% endfor %}
    </ul>
  </div>
{% endif %}