      "version_added": "11"
     },
     "opera_android": {
      "version_added": "11"
     },
     "safari": {
      "version_added": "6"
     },
     "safari_ios": {
      "notes": "Safari on iOS does not support indeterminate progress bars (they are rendered like 0%-completed progress bars).",
      "version_added": "7"
     },
